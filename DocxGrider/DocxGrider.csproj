<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net461;netstandard2.0</TargetFrameworks>
    <Title>DocxGrider</Title>
    <PackageId>DocxGrider</PackageId>
    <Version>1.3.1.0</Version>
    <Authors>liiws</Authors>
	<Description>DocxGrider is a very simple library for working with templates in .docx format.</Description>
    <PackageProjectUrl>https://github.com/liiws/DocxGrider</PackageProjectUrl>
    <RepositoryUrl>https://github.com/liiws/DocxGrider</RepositoryUrl>
    <PackageTags>docx;msword;word;openxml;office;template</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>icon.png</PackageIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Include="icon.png" Pack="true" PackagePath="" />
    <PackageReference Include="DocumentFormat.OpenXml" Version="3.2.0" />
  </ItemGroup>
</Project>
